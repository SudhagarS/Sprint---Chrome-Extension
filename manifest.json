{
  "name": "Sprint",
  "version": "0.3",
  "author": "Sudhagar",
  "description": "Just work, without distrations!",
  "permissions": ["webRequest", 
                  "webRequestBlocking",
                  "storage",
                  "alarms",
                  "https://*/*",
                  "http://*/*"],
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": { "128": "images/icon_128.png" },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon_inactive.png"
  }
}
